<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <title>Blog</title>
</head>
<body>
    <div class="container">  
        <h1 class="mb-4">Family Tree</h1>
        <a href="/identitas/new" class="btn btn-success" > Tambah Anggota Keluarga </a>

        <table class="table table-hover">
            <thead>
              <tr>
                <th>Nama Anggota Keluarga</th>
                <th>...</th>
              </tr>
            </thead>
            <tbody>
                <% identitas.forEach(identitas => { %>
                    <tr>
                        <td> <%= identitas.nama %></td>
                        <td> 
                            <a href="relasi_keluarga/<%= identitas.id %>" class="btn btn-primary">Detail</a>
                        </td>
                      </tr>
                <% }) %>
            </tbody>
          </table>

    </div>
</body>
</html>